include "src/bruter.pixi"

dl = dlopen( "data/main.so" ) //Open the library
if dl >= 0 
{
  f = dlsym( dl, "main", "p()" )
  t = dlcall( dl, f)
  printf( "main returned %s\n", t ) 
  bulk_interpret(t);
  
  while(1)
  {
    layers_render(variables.system.layers);
    eventor(variables.system);
    frame();
  }
  dlclose( dl ) //Close the library
}

