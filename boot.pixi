include "src/brute.pixi";

//main loop
//main loop
//main loop
fn main()
{
    $system = system_new();
    fn main_callback($system)
    {
        $log = get_system_log();
        print($log,-$system.config.width/2,0, WHITE, LEFT);
        remove($log);
    }
    keyboard_add($system.keyboard, KEY_F2, true, print_container_count);
    print_container_count();
    system_loop($system,main_callback, $system);
}

main();